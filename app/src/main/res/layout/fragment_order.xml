<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".fragment.OrderFragment">
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline27"
                                                android:orientation="vertical"
                                                app:layout_constraintGuide_percent="0.35"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline28"
                                                android:orientation="vertical"
                                                app:layout_constraintGuide_percent="0.65"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline26"
                                                android:orientation="vertical"
                                                app:layout_constraintGuide_percent="0.5"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline23"
                                                android:orientation="horizontal"
                                                app:layout_constraintGuide_percent="0.05"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline29"
                                                android:orientation="horizontal"
                                                app:layout_constraintGuide_percent="0.15"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline30"
                                                android:orientation="horizontal"
                                                app:layout_constraintGuide_percent="0.23"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline24"
                                                android:orientation="vertical"
                                                app:layout_constraintGuide_percent="0.05109489"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline25"
                                                android:orientation="vertical"
                                                app:layout_constraintGuide_percent="0.95"/>
    <TextView
            android:text="@string/order_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView12"
            app:layout_constraintStart_toStartOf="@+id/guideline27"
            app:layout_constraintEnd_toStartOf="@+id/guideline28" app:layout_constraintTop_toTopOf="@+id/guideline23"
            app:layout_constraintBottom_toTopOf="@+id/guideline23" android:textSize="@dimen/top_text_size"
            android:textStyle="bold"/>
    <ImageView
            android:src="@drawable/order_add"
            android:layout_width="67dp"
            android:layout_height="64dp" android:id="@+id/imageView10"
            app:layout_constraintDimensionRatio="w,1:1" app:layout_constraintStart_toStartOf="@+id/guideline24"
            app:layout_constraintEnd_toStartOf="@+id/guideline27" app:layout_constraintTop_toTopOf="@+id/guideline29"
            app:layout_constraintBottom_toTopOf="@+id/guideline29"/>
    <ImageView
            android:src="@drawable/order_list"
            android:layout_width="67dp"
            android:layout_height="64dp" android:id="@+id/imageView11"
            app:layout_constraintDimensionRatio="w,1:1"
            app:layout_constraintTop_toTopOf="@+id/guideline29" app:layout_constraintBottom_toTopOf="@+id/guideline29"
            app:layout_constraintEnd_toStartOf="@+id/guideline28"
            app:layout_constraintStart_toStartOf="@+id/guideline27"/>
    <ImageView
            android:src="@drawable/order_no_pay"
            android:layout_width="67dp"
            android:layout_height="64dp" android:id="@+id/imageView12"
            app:layout_constraintDimensionRatio="w,1:1" app:layout_constraintEnd_toStartOf="@+id/guideline25"
            app:layout_constraintStart_toStartOf="@+id/guideline28"
            app:layout_constraintTop_toTopOf="@+id/guideline29" app:layout_constraintBottom_toTopOf="@+id/guideline29"/>
    <TextView
            android:text="@string/order_add"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView13"
            app:layout_constraintEnd_toStartOf="@+id/guideline27"
            app:layout_constraintStart_toStartOf="@+id/guideline24"
            app:layout_constraintTop_toTopOf="@+id/guideline30" app:layout_constraintBottom_toTopOf="@+id/guideline30"
            android:textSize="@dimen/normal_size" android:textStyle="bold"/>
    <TextView
            android:text="@string/order_no_pay"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView14"
            app:layout_constraintEnd_toStartOf="@+id/guideline25"
            app:layout_constraintStart_toStartOf="@+id/guideline28"
            app:layout_constraintTop_toTopOf="@+id/guideline30" app:layout_constraintBottom_toTopOf="@+id/guideline30"
            android:textSize="@dimen/normal_size" android:textStyle="bold"/>
    <TextView
            android:text="@string/order_list"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView15"
            app:layout_constraintEnd_toStartOf="@+id/guideline28"
            app:layout_constraintStart_toStartOf="@+id/guideline27"
            app:layout_constraintTop_toTopOf="@+id/guideline30" app:layout_constraintBottom_toTopOf="@+id/guideline30"
            android:textSize="@dimen/normal_size" android:textStyle="bold"/>
</androidx.constraintlayout.widget.ConstraintLayout>