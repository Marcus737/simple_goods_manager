<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".fragment.GoodsListFragment">
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline401"
                                                android:orientation="vertical"
                                                app:layout_constraintGuide_percent="0.5"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline411"
                                                android:orientation="horizontal"
                                                app:layout_constraintGuide_percent="0.05"/>
    <TextView
            android:text="@string/inventory_list2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView181"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/guideline411" app:layout_constraintBottom_toTopOf="@+id/guideline411"
            android:textSize="@dimen/top_text_size" app:layout_constraintHorizontal_bias="0.444"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline421"
                                                android:orientation="horizontal"
                                                app:layout_constraintGuide_percent="0.1"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline441"
                                                android:orientation="horizontal"
                                                app:layout_constraintGuide_percent="0.74"/>
    <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/inventoryListRecycleView"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="@+id/guideline421"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toTopOf="@+id/guideline441"/>
</androidx.constraintlayout.widget.ConstraintLayout>